{
  "locals": {
    "url": "http://localhost:8080/",
    "title": "DOBT Knowledge Base",
    "section_sort_order": {
      "screendoor": [
        "projects",
        "your_form",
        "responses",
        "messages",
        "questions",
        "evaluation",
        "collaboration",
        "plans_and_billing",
        "integrations"
      ],
      "dashboard": [
        "accounts",
        "applications",
        "access"
      ],
      "adopta": [
        "site_admin"
      ],
      "govmapper": [
        "site_admin",
        "public_site"
      ]
    }
  },
  "require": {
    "_": "underscore"
  },
  "plugins": [
    "./plugins/sass.coffee",
    "wintersmith-coffee",
    "./plugins/nap.coffee",
    "./plugins/markdown-helper.coffee"
  ],
  "ignore": [
    "styles/includes/*",
    "**/.DS_Store"
  ],
  "markdown": {
    "smartypants": true,
    "gfm": true,
    "tables": true
  },
  "nap": {
    "assets": {
      "js": {
        "main": [
          "/js/flash_placeholder.js",
          "/js/service_status.coffee",
          "/js/styled_controls.coffee",
          "/js/styled_select.coffee",
          "/js/toggle_class.coffee",
          "/js/purl.js",
          "/js/jquery.placeholder.js",
          "/js/jquery.fitvids.js",
          "/js/main.coffee"
        ]
      }
    }
  }
}
